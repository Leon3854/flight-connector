{
  "name": "flight-connector",
  "version": "1.0.0",
  "description": "The flight connector for the Travel API",
  "main": "dist/app.js",
  "type": "commonjs",
  "exports": {
    "./src/config/knex/knexfile.js": "./src/config/knex/knexfile.ts"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc && npm run postbuild",
    "postbuild": "mkdir -p dist/config/knex dist/database && cp -r src/config/knex/* dist/config/knex/ && cp -r src/database/* dist/database/",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:docker": "docker-compose run --rm test-runner",
    "test:docker:unit": "docker-compose run -e NODE_ENV=test --rm test-runner npm test",
    "migrate:docker": "docker-compose run --rm migrate",
    "test:clean": "docker-compose down -v --remove-orphans",
    "prebuild": "rm -rf dist/",
    "migrate:make": "knex migrate:make -x ts --knexfile=src/config/knex/knexfile.ts",
    "migrate": "knex migrate:latest --knexfile=src/config/knex/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile=src/config/knex/knexfile.ts",
    "seed": "knex seed:run --knexfile=src/config/knex/knexfile.ts",
    "dev": "nodemon --exec ts-node src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Leon3854/flight-connector.git"
  },
  "keywords": [],
  "author": "Frad3854",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Leon3854/flight-connector/main"
  },
  "homepage": "https://github.com/Leon3854/flight-connector#readme",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.122",
    "cross-env": "^10.0.0",
    "inversify": "^7.7.1",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "redis": "^5.6.1",
    "zod": "^4.0.10"
  }
}
